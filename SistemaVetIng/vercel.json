{
  "version": 2,
  "builds": [
    {
      "src": "SistemaVetIng.csproj",
      "use": "jamieleslie/vercel-csharp@latest"
    }
  ],
  "routes": [
    {
      "src": "/wwwroot/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000, immutable"
      },
      "dest": "/wwwroot/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "SistemaVetIng.csproj"
    }
  ]
}