@using X.PagedList.Mvc.Core
@model SistemaVetIng.ViewsModels.VeterinarioPaginaPrincipalViewModel
@using Microsoft.AspNetCore.Authorization
@inject IAuthorizationService AuthorizationService
@inject IHttpContextAccessor HttpContextAccessor
@using SistemaVetIng.Models.Extension

@{
    ViewData["Title"] = "VetIng - Veterinario";
    Layout = null;
}
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css"
          integrity="sha512-1sCRPdkRXhBV2PBLUdRb4tMg1w2YPf37qatUFeS7zlBy7jJI8Lf4VHwWfZZfpXtYSLy85pkm9GaYVYMfw5BC1A=="
          crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="~/css/veterinario-home.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/administracion-home.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/veterinary-dashboard.css" asp-append-version="true" />
</head>

<body>
    <header class="main-header">
        <a href="/Veterinario/PaginaPrincipal" class="logo"><i class="fa-solid fa-paw"></i> VetIng</a>
        <nav class="main-nav">
            <ul class="nav-list">
                 <li>
                 <a class="nav-link" asp-controller="Perfil" asp-action="Index">Perfil</a>
                </li>
                <li><a class="nav-link" href="#resumen">Resumen</a></li>
                <li><a class="nav-link" href="#clientes">Clientes</a></li>
                <li><a class="nav-link" href="#mascotas">Mascotas</a></li>
                <li><a class="nav-link" href="#seguimiento-clinico">Seguimiento Clínico</a></li>
                <li>
                    <form asp-controller="Account" asp-action="Logout" method="post" id="logoutForm">
                        <button type="submit" class="btn sesion nav-button">Cerrar Sesión</button>
                    </form>
                </li>
            </ul>
        </nav>
        <div class="hamburger-menu">
            <i class="fa-solid fa-bars"></i>
        </div>
    </header>

    <main class="v1-main-dashboard">

        <section id="resumen" class="v1-section">
            <h2 class="v1-section-title">Resumen del Día</h2>
            <div class="v1-card-grid">
                <div class="v1-card">
                    <div class="v1-card-header">
                        <h3 class="v1-card-title">Citas</h3>
                        <i class="fa-regular fa-calendar-days v1-card-icon"></i>
                    </div>
                    <div class="v1-card-content">
                        <div class="v1-card-value">
                           @Model.CantidadCitasHoy
                        </div>
                          <p class="v1-card-description">
                           Para Hoy
                         </p>
                    </div>
                </div>
                <div class="v1-card">
                    <div class="v1-card-header">
                        <h3 class="v1-card-title">Mascotas Atendidas</h3>
                        <i class="fa-solid fa-paw v1-card-icon"></i>
                    </div>
                    <div class="v1-card-content">
                        <div class="v1-card-value">
                            @Model.CantidadAtencionesPorVeterinario
                        </div>
                          <p class="v1-card-description">
                            Historicamente
                         </p>
                    </div>
                </div>
                <div class="v1-card">
                    <div class="v1-card-header">
                        <h3 class="v1-card-title">Mascota mas Frecuente</h3>
                        <i class="fa-solid fa-file-invoice-dollar v1-card-icon"></i>
                    </div>
                    <div class="v1-card-content">
                        <div class="v1-card-value">
                            @(Model.MascotaMasFrecuentePorVeterinario?.Nombre ?? "N/A")
                        </div>
                        <p class="v1-card-description">
                            @(Model.MascotaMasFrecuentePorVeterinario?.Especie ?? "N/A")
                            @(Model.MascotaMasFrecuentePorVeterinario?.Raza ?? "N/A")
                         </p>
                    </div>
                </div>
            </div>
        </section>

      


        @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Administration.ViewRoles)).Succeeded ||
        (await AuthorizationService.AuthorizeAsync(User, Permission.Administration.ManageRolePermissions)).Succeeded ||
        (await AuthorizationService.AuthorizeAsync(User, Permission.Administration.ViewUsers)).Succeeded)
        {
            <section id="administracion" class="v1-section">
                <h2 class="v1-section-title">Administración del Sistema</h2>
                <div class="v1-section-actions">

                    @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Administration.ManageRolePermissions)).Succeeded)
                    {
                        <a asp-controller="Veterinaria" asp-action="GestionPermissions" class="v1-btn v1-btn-primary">
                            <i class="fa-solid fa-shield-halved"></i> Gestionar Permisos de Roles
                        </a>
                    }

                </div>
                <div class="v1-section-actions">
                    @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Administration.ViewUsers)).Succeeded)
                    {
                        <a asp-controller="Veterinaria" asp-action="GestionPermissionsUsuario" class="v1-btn v1-btn-primary">
                            <i class="fa-solid fa-users-cog"></i> Gestionar Usuarios
                        </a>
                    }
                </div>
                <div class="v1-section-actions">
                    @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Administration.ViewRoles)).Succeeded)
                    {
                        <a asp-controller="Veterinaria" asp-action="GestionRolesUsuario" class="v1-btn v1-btn-primary">
                            <i class="fa-solid fa-user-shield"></i> Gestionar Roles
                        </a>
                    }
                </div>

            </section>
        }

        @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Horario.View)).Succeeded){
        <section id="configuracion-turnos" class="v1-section">
            <h2 class="v1-section-title">Configuración de Horarios</h2>
            <div class="v1-section-actions">
                @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Horario.Create)).Succeeded)
                {
                    <a asp-controller="Veterinaria" asp-action="GuardarConfiguracion" class="v1-btn v1-btn-primary">
                        <i class="fas fa-cog"></i> Agregar/Modificar Configuración
                    </a>
                }
            </div>
            <div class="v1-card">
                <div class="v1-card-header">
                    <h3 class="v1-card-title">Configuración Actual</h3>
                </div>
                <div class="v1-card-content">
                    @if (Model.ConfiguracionTurnos != null && Model.ConfiguracionTurnos.Horarios.Any())
                    {
                        <div class="v1-info-item" style="margin-bottom: 20px;">
                            <strong>Duración por Consulta:</strong>
                            <span>@Model.ConfiguracionTurnos.DuracionMinutosPorConsulta minutos</span>
                        </div>

                        <table class="v1-table">
                            <thead>
                                <tr>
                                    <th>Día de la Semana</th>
                                    <th>Estado</th>
                                    <th>Horario Laboral</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var dia in Model.ConfiguracionTurnos.Horarios)
                                {
                                    <tr>
                                        <td>

                                            @switch (dia.DiaSemana)
                                            {
                                                case DayOfWeek.Monday:
                                                    <span>Lunes</span>
                                                    break;
                                                case DayOfWeek.Tuesday:
                                                    <span>Martes</span>
                                                    break;
                                                case DayOfWeek.Wednesday:
                                                    <span>Miércoles</span>
                                                    break;
                                                case DayOfWeek.Thursday:
                                                    <span>Jueves</span>
                                                    break;
                                                case DayOfWeek.Friday:
                                                    <span>Viernes</span>
                                                    break;
                                                case DayOfWeek.Saturday:
                                                    <span>Sábado</span>
                                                    break;
                                                case DayOfWeek.Sunday:
                                                    <span>Domingo</span>
                                                    break;
                                            }
                                        </td>
                                        <td>
                                            @if (dia.EstaActivo)
                                            {
                                                <span class="v1-badge v1-badge-success">Activo</span>
                                            }
                                            else
                                            {
                                                <span class="v1-badge v1-badge-danger">Inactivo</span>
                                            }
                                        </td>
                                        <td>

                                            @if (dia.EstaActivo && dia.HoraInicio != DateTime.MinValue)
                                            {

                                                <span>@dia.HoraInicio.ToString("HH:mm") - @dia.HoraFin.ToString("HH:mm")</span>
                                            }
                                            else
                                            {
                                                <span>-</span>
                                            }
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <div class="v1-empty-state">Aún no se ha configurado el horario.</div>
                    }
                </div>
            </div>
        </section>
        }

       
        <section id="catalogos" class="v1-section">
            @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Vacuna.View)).Succeeded){
            <h2 class="v1-section-title">Gestión de Servicios</h2>
            <div class="v1-section-actions">
                @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Vacuna.Create)).Succeeded)
                {
                    <a asp-controller="Vacuna" asp-action="Index" class="v1-btn v1-btn-primary">
                        <i class="fa-solid fa-syringe"></i> Gestionar Catálogo de Vacunas
                    </a>
                }
            </div>
            }
             @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Estudio.View)).Succeeded){
            <div class="v1-section-actions">
                @if ((await AuthorizationService.AuthorizeAsync(User, Permission.Estudio.Create)).Succeeded)
                {
                    <a asp-controller="Estudio" asp-action="Index" class="v1-btn v1-btn-primary">
                        <i class="fa-solid fa-microscope"></i> Gestionar Catálogo de Estudios
                    </a>
                }
            </div>
             }
        </section>

        @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Turnos.View))
        {
        <section id="turnos" class="v1-section">
                <div class="v1-card v1-appointment-list-card">
                    <div class="v1-card-header">
                          <h3 class="v1-card-title">Citas programadas para el @DateTime.Today.ToString("dd/MM/yyyy")</h3>
                              <a asp-controller="Turno" asp-action="TurnosPorDiaSeleccionado" class="v1-btn v1-btn-primary">
                            <i class="fa-solid fa-calendar"></i> Ver Todos los Turnos
                          </a> 
                    </div>
                    <div class="v1-card-content v1-appointment-list-content">

                        @if (Model.CitasDeHoy.Any())
                        {
                            @foreach (var turno in Model.CitasDeHoy)
                            {
                                <div class="v1-appointment-item">
                                    <div class="v1-appointment-info">
                                        <i class="fa-regular fa-clock v1-appointment-icon"></i>
                                        <div>
                                            <p class="v1-appointment-time">@turno.Horario.ToString(@"hh\:mm") hs</p>

                                            @if (turno.PrimeraCita)
                                            {
                                                <p class="v1-appointment-pet primeraCita">Primera Cita</p>
                                            }
                                            else
                                            {
                                                <p class="v1-appointment-pet">Mascota: @(turno.NombreMascota ?? "Sin mascota")</p>
                                            }

                                            <p class="v1-appointment-owner">Cliente: @turno.NombreCliente</p>
                                        </div>
                                    @if (turno.Estado == "Pendiente")
                                    {
                                        <div class="v1-appointment-actions">
                                            <a asp-controller="AtencionVeterinaria" asp-action="RegistrarAtencionConTurno" asp-route-turnoId="@turno.Id" class="v1-btn v1-btn-primary">
                                                <i class="fa-solid fa-file-waveform"></i> Registrar Atención
                                            </a>
                                        </div>
                                            @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Turnos.Cancel))
                                            {
                                        <form asp-controller="Turno" asp-action="CancelarTurnoAdmin" method="post" onsubmit="return confirm('¿Estás seguro de cancelar este turno?');" style="display: inline-block; margin-left: 5px;">
                                            @Html.AntiForgeryToken()
                                            <input type="hidden" name="turnoId" value="@turno.Id" />
                                            <button type="submit" class="v1-btn-cancelar">
                                                <i class="fas fa-calendar-times"></i> Cancelar
                                            </button>
                                        </form>
                                            }
                                        <form asp-controller="Turno" asp-action="MarcarNoAsistio" method="post" onsubmit="return confirm('¿Confirmar que el cliente no asistió a este turno?');" style="display: inline-block; margin-left: 5px;">
                                            @Html.AntiForgeryToken()
                                            <input type="hidden" name="turnoId" value="@turno.Id" />
                                            <button type="submit" class="v1-btn-ausente">
                                                <i class="fas fa-user-slash"></i> Ausente
                                            </button>
                                        </form>
                                    }
                                    </div>
                                    <div class="v1-appointment-badges">
                                        @if (!string.IsNullOrWhiteSpace(turno.Motivo))
                                        {
                                            <span class="v1-badge v1-badge-secondary">@turno.Motivo</span>
                                        }

                                   <span class="v1-badge @(turno.Estado == "Finalizado" ? "v1-badge-green" : (turno.Estado == "Pendiente" ? "v1-badge-yellow" : (turno.Estado == "Cancelado" || turno.Estado == "No Asistió" ? "v1-badge-red" : "v1-badge-gray"))) ">
                                        @turno.Estado
                                    </span>
                                    </div>
                                </div>
                            }
                        }
                        else
                        {
                            <div class="v1-empty-state">No hay citas programadas para hoy.</div>
                        }
                    </div>
                </div>
        </section>
        }

        @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Cliente.View)){
        <section id="clientes" class="v1-section">
            <h2 class="v1-section-title">Gestión de Clientes</h2>
            <div class="v1-section-actions">
                    @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Cliente.Create))
                    {
                <a asp-controller="Cliente" asp-action="RegistrarCliente" class="v1-btn v1-btn-primary">
                    <i class="fa-solid fa-plus"></i> Registrar Nuevo
                </a>
                    }

                <form asp-controller="Veterinario" asp-action="PaginaPrincipal" method="get" class="v1-search-form">
                    <div class="v1-input-group">
                        <i class="fa-solid fa-search v1-input-icon"></i>
                        <input type="text" id="busquedaCliente" name="busquedaCliente" placeholder="Buscar por DNI, Nombre..." class="v1-input" value="@Context.Request.Query["busquedaCliente"]" />
                    </div>
                </form>
            </div>
            <div class="v1-card">
                <div class="v1-card-header">
                    <h3 class="v1-card-title">Lista de Clientes</h3>
                </div>
                <div class="v1-card-content v1-list-content">
                    @if (Model.Clientes != null && Model.Clientes.Any())
                    {
                        @foreach (var cliente in Model.Clientes)
                        {
                            <div class="v1-list-item">
                                <div class="v1-list-info">
                                    <div class="v1-avatar">@(cliente.NombreCompleto.Split(' ').FirstOrDefault()?.FirstOrDefault())@(cliente.NombreCompleto.Split(' ').LastOrDefault()?.FirstOrDefault())</div>
                                    <div>
                                        <p class="v1-list-name">@cliente.NombreCompleto</p>
                                        <p class="v1-list-subtext">Dni: @cliente.DNI</p>
                                        <p class="v1-list-subtext-small">Telefono: @cliente.Telefono</p>
                                        <p class="v1-list-subtext-small">Usuario: @cliente.NombreUsuario</p>
                                    </div>
                                </div>
                                <div class="v1-list-actions">
                                        @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Cliente.Edit))
                    {
                                    <a asp-controller="Cliente" asp-action="ModificarCliente" asp-route-id="@cliente.Id" class="v1-btn v1-btn-ghost"><i class="fa-solid fa-edit"></i></a>
                    }
                                        @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Cliente.Delete))
                                        {
                                    <form asp-controller="Cliente" asp-action="EliminarCliente" asp-route-id="@cliente.Id" method="post" onsubmit="return confirm('¿Estás seguro de que quieres eliminar a este cliente?');" style="display:inline;">
                                        @Html.AntiForgeryToken() 
                                        <button type="submit" class="v1-btn v1-btn-ghost v1-text-danger"><i class="fa-solid fa-trash"></i></button>
                                    </form>
                                        }
                                </div>
                            </div>
                        }
                    }
                    else
                    {
                        <div class="v1-empty-state">
                            @if (!string.IsNullOrEmpty(Context.Request.Query["busquedaCliente"]))
                            {
                                <span>No se encontraron clientes que coincidan con "@Context.Request.Query["busquedaCliente"]".</span>
                            }
                            else
                            {
                                <span>No hay clientes registrados.</span>
                            }
                        </div>
                    }
                </div>

                <div class="v1-card-footer pagination-container">
                    @if (Model.PaginacionClientes != null && Model.PaginacionClientes.PageCount > 1)
                    {
                        @Html.PagedListPager(Model.PaginacionClientes, page => Url.Action("PaginaPrincipal", "Veterinario",
                        new { page, busquedaCliente = Context.Request.Query["busquedaCliente"]}),
                                        new PagedListRenderOptions
                                        {
                                            LiElementClasses = new string[] { "page-item" },
                                            PageClasses = new string[] { "page-link" },
                                            DisplayLinkToFirstPage = PagedListDisplayMode.IfNeeded,
                                            DisplayLinkToLastPage = PagedListDisplayMode.IfNeeded,
                                            DisplayLinkToPreviousPage = PagedListDisplayMode.Always,
                                            DisplayLinkToNextPage = PagedListDisplayMode.Always,
                                            ContainerDivClasses = new[] { "pagination justify-content-center" }
                                        })
                                        }
                    else if (Model.PaginacionClientes != null)
                    {
                        <p class="text-muted text-center">Mostrando @Model.PaginacionClientes.TotalItemCount @(Model.PaginacionClientes.TotalItemCount == 1 ? "cliente" : "clientes")</p>
                    }
                </div>
            </div>
        </section>
        }

        @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Mascota.View)){
     <section id="mascotas" class="v1-section">
    <h2 class="v1-section-title">Gestión de Mascotas</h2>
    <div class="v1-section-actions">
                    @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Mascota.Create))
                    {
         <a asp-controller="Mascota" asp-action="RegistrarMascota" class="v1-btn v1-btn-primary">
            <i class="fa-solid fa-plus"></i> Registrar Nueva
        </a> 
                    }

        <form asp-controller="Veterinario" asp-action="PaginaPrincipal" method="get" class="v1-search-form">
            <div class="v1-input-group">
                <i class="fa-solid fa-search v1-input-icon"></i>
                <input type="text" id="busquedaMascota" name="busquedaMascota" placeholder="Buscar por Nombre, Raza, Dueño..." class="v1-input" value="@Context.Request.Query["busquedaMascota"]" />
            </div>
        </form>
    </div>
    <div class="v1-card">
        <div class="v1-card-header">
            <h3 class="v1-card-title">Registro de Mascotas</h3>
        </div>
        <div class="v1-card-content v1-list-content">
            @if (Model.Mascotas != null && Model.Mascotas.Any())
            {
                @foreach (var mascota in Model.Mascotas) 
                {
                    <div class="v1-list-item">
                        <div class="v1-list-info">
                            <div class="v1-pet-icon-bg"> 
                                @if (mascota.Especie.ToLower().Contains("canino") || mascota.Especie.ToLower().Contains("perro")) {
                                    <i class="fa-solid fa-dog v1-pet-icon"></i>
                                } else if (mascota.Especie.ToLower().Contains("felino") || mascota.Especie.ToLower().Contains("gato")) {
                                    <i class="fa-solid fa-cat v1-pet-icon"></i>
                                } else {
                                    <i class="fa-solid fa-paw v1-pet-icon"></i> 
                                }
                            </div>
                            <div>
                                <p class="v1-list-name">@mascota.NombreMascota</p>
                                <p class="v1-list-subtext">@mascota.Especie &bull; @mascota.Raza (@mascota.Sexo)</p>
                                <p class="v1-list-subtext-small">Edad: @mascota.EdadAnios @(mascota.EdadAnios == 1 ? "año" : "años")</p>
                                <p class="v1-list-subtext-small">Propietario: @mascota.NombreDueno</p>
                            </div>
                        </div>
                        <div class="v1-list-actions">
                             <a asp-controller="HistoriaClinica" asp-action="DetalleHistoriaClinica" asp-route-mascotaId="@mascota.Id" class="v1-btn v1-btn-ghost" title="Ver Historia Clínica">
                                 <i class="fa-solid fa-notes-medical"></i>
                             </a>
                                        @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Mascota.Edit))
                                        {
                            <a asp-controller="Mascota" asp-action="ModificarMascota" asp-route-id="@mascota.Id" class="v1-btn v1-btn-ghost" title="Editar Mascota"><i class="fa-solid fa-edit"></i></a>
                             }
                                        @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Mascota.Delete))
                                        {
                            <form asp-controller="Mascota" asp-action="EliminarMascota" asp-route-id="@mascota.Id" method="post" onsubmit="return confirm('¿Estás seguro de que quieres eliminar a esta mascota?');" style="display:inline;">
                                @Html.AntiForgeryToken()
                                <button type="submit" class="v1-btn v1-btn-ghost v1-text-danger" title="Eliminar Mascota"><i class="fa-solid fa-trash"></i></button>
                            </form>
                            }
                        </div>
                    </div>
                }
            }
            else
            {
                <div class="v1-empty-state">
                    @if (!string.IsNullOrEmpty(Context.Request.Query["busquedaMascota"]))
                    {
                        <span>No se encontraron mascotas que coincidan con "@Context.Request.Query["busquedaMascota"]".</span>
                    }
                    else
                    {
                        <span>No hay mascotas registradas.</span>
                    }
                </div>
            }
        </div>

        <div class="v1-card-footer pagination-container"> 
            @if (Model.PaginacionMascotas != null && Model.PaginacionMascotas.PageCount > 1) 
            {
                @Html.PagedListPager(Model.PaginacionMascotas, pageMascota => Url.Action("PaginaPrincipal", "Veterinario",
                    new { 
                        page = Context.Request.Query["page"], 
                        busquedaCliente = Context.Request.Query["busquedaCliente"],
                        pageMascota,
                        busquedaMascota = Context.Request.Query["busquedaMascota"] 
                    }),
                    new PagedListRenderOptions
                    {
                          LiElementClasses = new string[] { "page-item" },
                                            PageClasses = new string[] { "page-link" },
                                            DisplayLinkToFirstPage = PagedListDisplayMode.IfNeeded,
                                            DisplayLinkToLastPage = PagedListDisplayMode.IfNeeded,
                                            DisplayLinkToPreviousPage = PagedListDisplayMode.Always,
                                            DisplayLinkToNextPage = PagedListDisplayMode.Always,
                                            ContainerDivClasses = new[] { "pagination justify-content-center" }
                    })
            }
            else if(Model.PaginacionMascotas != null)
            {
                 <p class="text-muted text-center">Mostrando @Model.PaginacionMascotas.TotalItemCount @(Model.PaginacionMascotas.TotalItemCount == 1 ? "mascota" : "mascotas")</p>
            }
        </div>
    </div>
</section>
}
        @if (HttpContextAccessor.HttpContext.User.HasPermission(Permission.Atenciones.View))
        {
        <section id="seguimiento-clinico" class="v1-section">
            <h2 class="v1-section-title">Seguimiento Clínico</h2>
            <div class="v1-section-actions">
